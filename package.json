{
  "name": "resumeapp.friedunix.net",
  "version": "1.7.5",
  "description": "Resume for Alex Friedrichsen written primarily in Node.js",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.1",
    "chalk": "^2.3.2",
    "coffee-script": "^1.x",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.15.4",
    "express-validation": "^1.0.2",
    "helmet": "^3.12.0",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "https": "^1.0.0",
    "jade": "^0.35.x",
    "joi": "^13.1.2",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "moment": "^2.22.0",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0",
    "npm": "^5.8.0",
    "object-assign": "^4.1.1",
    "path": "^0.12.7",
    "pm2": "^2.9.1",
    "promise": "^8.0.1",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "uglify-js": "^3.3.20",
    "which": "^1.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/app/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:2112",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/app/api/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/app/api/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "nodemon": "^1.13.3",
    "nyc": "^11.6.0",
    "should": "^13.0.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive app/api/tests",
    "validate": "yarn lint && yarn test",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint:watch": "yarn lint -- --watch",
    "development": "nodemon ./app/index.js",
    "production": "pm2 start ./app/app.js",
    "start": "node ./app/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afriedrichsen/resumeApp.git"
  },
  "keywords": [
    "Node",
    "resume"
  ],
  "author": "Alexander (Alex) Friedrichsen",
  "license": "ISC",
  "homepage": "https://github.com/afriedrichsen/resumeAppDev.friedunix.net#readme",
  "bugs": {
    "url": "https://github.com/afriedrichsen/resumeApp/issues"
  }
}
