{
  "name": "resumeapp.friedrichsen.me",
  "version": "2.6.0",
  "description": "Resume for Alex Friedrichsen written primarily in Node.js",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.1",
    "chalk": "^2.3.2",
    "coffeescript": "^1.x",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.15.4",
    "express-validation": "^1.0.2",
    "helmet": "^3.12.0",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "https": "^1.0.0",
    "joi": "^13.1.2",
    "lodash": "^4.17.15",
    "method-override": "^2.3.10",
    "moment": "^2.22.0",
    "mongoose": "^5.6.13",
    "morgan": "^1.9.1",
    "npm": "^6.11.3",
    "object-assign": "^4.1.1",
    "path": "^0.12.7",
    "pm2": "^2.9.1",
    "prettier": "^1.18.2",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "uglify-js": "^3.3.20",
    "which": "^1.x"
  },
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.18",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.3.1",
    "@types/mongoose": "^5.5.17",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.4",
    "@types/serve-favicon": "^2.2.31",
    "@types/supertest": "^2.0.8",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jest": "^25.0.0",
    "jest-cli": "^25.0.0",
    "micromatch": "^3.1.10",
    "mocha": "^5.2.0",
    "nodemon": "^1.13.3",
    "nyc": "^14.1.1",
    "should": "^13.0.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive app/api/tests",
    "test:v2": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "validate": "yarn lint && yarn test",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint:watch": "yarn lint -- --watch",
    "development": "nodemon ./app/index.js",
    "production": "pm2 start ./app/app.js",
    "start": "node ./app/index.js",
    "build:v2": "tsc && npm run copy:static",
    "copy:static": "cp -r ./app/public/ ./dist/public",
    "start_dev_local": "nodemon -L --exec ./node_modules/.bin/ts-node -- ./app/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afriedrichsen/resumeApp.git"
  },
  "keywords": [
    "Node",
    "resume"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:2112",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "testRegex": "(/tests/.*(\\.|/)spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "ts.d"
    ]
  },
  "author": "Alexander (Alex) Friedrichsen",
  "license": "ISC",
  "homepage": "https://github.com/afriedrichsen/resumeApp#readme",
  "bugs": {
    "url": "https://github.com/afriedrichsen/resumeApp/issues"
  }
}
